	if val == true then
		local BFDOF = Instance.new("DepthOfFieldEffect", LG)
		BFDOF.Name = "BF_DOF"
		LG.BF_DOF.FarIntensity = 0
		LG.BF_DOF.FocusDistance = 0
		LG.BF_DOF.InFocusRadius = 0
		LG.BF_DOF.NearIntensity = 0
	else
		LG.BF_DOF:Destroy()
	end
end)

