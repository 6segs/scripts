--Another World Features Text
world_tab:Label("                    Another World Settings:")

--NightMode
world_tab:Toggle("NightMode", false, function(val)
    if val == true then
		LG.TimeOfDay = 20
	else
		LG.TimeOfDay = 13
	end
end)

--Remove Rain on Aztec map
world_tab:Button("Remove Rain on Aztec map", function()
	game.Workspace.Map.Geometry.RainParts:Destroy()
end)

world_tab:Seperator()

world_tab:Label("                    Skies:")

world_tab:Toggle("Skybox Changer", false, function(val)
	if val == true then
		local boxsky = Instance.new("Sky", LG)
		wait(1)
		boxsky.Name = "BFSky"
		wait(1)
		print("Skyboxes Turned On")
	else
		print("Skyboxes Turned Off")
		wait(1)
		LG.BFSky:Destroy()
	end
lib:Notification("ReadMe!", "Wait a few seconds to Skybox load when you change it.", "[OKAY]")
end)

