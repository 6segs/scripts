--[[ --Exapmles=--------------------------------------------------------------------------
Home:Seperator()

Home:Button("Discord Invite", function()
	setclipboard("https://discord.gg/EjxDawbZKu")
end)
Home:Button("Rejoin Server", function()
	game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId)
end)
Home:Button("Destroy GUI", function()
    game.CoreGui.Discord:Destroy()
end)
Home:Seperator()
--Close BloxFun Bind
Home:Bind("Close BloxFun Bind",Enum.KeyCode.LeftAlt, function()
	game.CoreGui.Discord.Enabled = false
end)

--Open BloxFun Bind
Home:Bind("Open BloxFun Bind",Enum.KeyCode.RightAlt, function()
	game.CoreGui.Discord.Enabled = true
end)
--]]
