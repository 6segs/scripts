--Field Of View
cheats_tab:Slider("Field Of View",60,120,80, function(val)
	game.Workspace.CurrentCamera.FieldOfView = val
	getsenv(game.Players.LocalPlayer.PlayerGui.Client).fieldofview = val
end)

cheats_tab:Seperator()

--No Flash
cheats_tab:Toggle("No Flash", false, function(val)
    if val == true then
		game.Players.LocalPlayer.PlayerGui.Blnd.Enabled = false
	else
		game.Players.LocalPlayer.PlayerGui.Blnd.Enabled = true
	end
end)

cheats_tab:Seperator()

--Remove Scope
cheats_tab:Toggle("Remove Scope", false, function(val)
    if val == true then
		game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Scope.ImageTransparency = 1
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Scope.Scope.ImageTransparency = 1
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Scope.Scope.Blur.ImageTransparency = 1
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Scope.Scope.Blur.Blur.ImageTransparency = 1
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Frame1.Transparency = 1
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Frame2.Transparency = 1
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Frame3.Transparency = 1
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Frame4.Transparency = 1
	else
		game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Scope.ImageTransparency = 0
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Scope.Scope.ImageTransparency = 0
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Scope.Scope.Blur.ImageTransparency = 1
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Scope.Scope.Blur.Blur.ImageTransparency = 1
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Frame1.Transparency = 0
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Frame2.Transparency = 0
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Frame3.Transparency = 0
        game.Players.LocalPlayer.PlayerGui.GUI.Crosshairs.Frame4.Transparency = 0
    end
end)

cheats_tab:Seperator()

--Spectators List Engine
loadstring(game:HttpGet("https://raw.githubusercontent.com/NineR-9R/BloxFun/main/spectators-list.lua", true))()
game.CoreGui.SpectatorsList.Enabled = false

--Open venosu's Spectators List
cheats_tab:Bind("Open venosu's Spectators List",Enum.KeyCode.I, function()
	game.CoreGui.SpectatorsList.Enabled = true
end)

--Close venosu's Spectators List
cheats_tab:Bind("Close venosu's Spectators List",Enum.KeyCode.O, function()
	game.CoreGui.SpectatorsList.Enabled = false
end)

cheats_tab:Seperator()

--No Crouch Cooldown
cheats_tab:Toggle("No Crouch Cooldown", false, function(val)
    if val == true then
		game:GetService("RunService"):BindToRenderStep("NoCrouchCooldown", 100, function()
			cbClient.crouchcooldown = 0
		end)
	else
		game:GetService("RunService"):UnbindFromRenderStep("NoCrouchCooldown")
	end
end)

cheats_tab:Seperator()

--Auto Join Winning Team
cheats_tab:Toggle("Auto Join Winning Team", false, function(val)
	if val == true then
		if data.TWins.Value > data.CTWins.Value and plr.Team ~= 'Terrorists' then
			team:FireServer('T')
		elseif plr.Team ~= 'Counter-Terrorists' then
			team:FireServer('CT')
		end
	end
end)

cheats_tab:Seperator()

--Drop Pistols
cheats_tab:Dropdown("Drop Pistols",{"PX4","USP-S","Glock-18","Dual Berettas","Five-Seven","TEC-9","CZ75-Auto","P250","Deagle","Revolver"}, function(gundrops)
if gundrops == "PX4" then
        game.ReplicatedStorage.Events.Drop:FireServer(game.ReplicatedStorage.Weapons.P2000, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 999999, 99999, false, game.Players.LocalPlayer, false, false)
elseif gundrops == "USP-S" then
    game.ReplicatedStorage.Events.Drop:FireServer(game.ReplicatedStorage.Weapons.USP, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 999999, 99999, false, game.Players.LocalPlayer, false, false)
