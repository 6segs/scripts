    game.ReplicatedStorage.Events.Drop:FireServer(game.ReplicatedStorage.Weapons.Tec9, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 999999, 99999, false, game.Players.LocalPlayer, false, false)
