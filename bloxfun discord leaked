--=================================================================================================================================
--   Maps Tab
maps_tab:Seperator()

maps_tab:Toggle("hw_mirage", false, function(val)
	if val == true then
		game.Workspace.Map:Destroy()
		wait(1)
		local Model9159 = Instance.new("Model", game.Workspace)
		game:GetObjects('rbxassetid://6411595443')[1].Parent = Model9159
		wait(1)
		Model = workspace.Model
		for _, Child in pairs(Model:GetChildren()) do
			Child.Parent = Model.Parent
		end
		Model:Destroy()
	else
		game.Workspace.Map:Destroy()
	end
end)

maps_tab:Seperator()

