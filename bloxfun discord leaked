    game.ReplicatedStorage.Events.Drop:FireServer(game.ReplicatedStorage.Weapons.FiveSeven, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 999999, 99999, false, game.Players.LocalPlayer, false, false)
