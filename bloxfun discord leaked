world_tab:Label("                    Lighting Settings:")

--Ambient / WorldColor
world_tab:Colorpicker("Ambient / WorldColor)",Color3.fromRGB(90, 74, 62), function(val)
    LG.Ambient = val
end)

--Brightness
world_tab:Textbox("Brightness", "Type here!", true, function(val)
	LG.Brightness = val;
end)

--ClockTime
world_tab:Textbox("Clock Time", "Sleepy Sleepy", true, function(val)
	LG.ClockTime = val;
end)

--ColorShift_Bottom
world_tab:Colorpicker("ColorShift_Bottom",Color3.fromRGB(0, 0, 0), function(val)
    LG.ColorShift_Bottom = val
end)

--ColorShift_Top
world_tab:Colorpicker("ColorShift_Top",Color3.fromRGB(0, 0, 0), function(val)
    LG.ColorShift_Top = val
end)

--FogColor
world_tab:Colorpicker("FogColor",Color3.fromRGB(191, 191, 191), function(val)
    LG.FogColor = val
end)

--FogEnd
world_tab:Textbox("Fog End", "Number", true, function(val)
	LG.FogEnd = val;
end)

--FogStart
world_tab:Textbox("Fog Start", "Number", true, function(val)
	LG.FogStart = val;
end)

--GeographicLatitude
world_tab:Textbox("GeographicLatitude", "Number", true, function(val)
	LG.GeographicLatitude = val;
end)

--Global Shadows
world_tab:Toggle("Global Shadows", true, function(val)
    if val == true then
		LG.GlobalShadows = true
	else
		LG.GlobalShadows = false
	end
end)

--Outdoor Ambient
world_tab:Colorpicker("Outdoor Ambient",Color3.fromRGB(165, 156, 140), function(val)
    LG.OutdoorAmbient = val
end)

--TimeOfDay
world_tab:Textbox("Time of Day", "Change Time", true, function(val)
	LG.TimeOfDay = val;
end)

world_tab:Seperator()

-- Effects Configuration Text
world_tab:Label("                    Effects Configuration:")


--  Color Correction Effect
world_tab:Toggle("Color Correction Effect", false, function(val)
	if val == true then
		local BFCCE = Instance.new("ColorCorrectionEffect", LG)
		BFCCE.Name = "BF_CCE"
		LG.BF_CCE.Saturation = 0
		LG.BF_CCE.Brightness = 0
		LG.BF_CCE.Contrast = 0
		LG.BF_CCE.TintColor = 255, 255, 255
	else
		LG.BF_CCE:Destroy()
	end
end)

--Saturation (Color Correction)
world_tab:Textbox("Saturation", "(Color Correction)", true, function(val)
	LG.BF_CCE.Saturation = val;
end)

--Brightness (Color Correction)
world_tab:Textbox("Brightness", "(Color Correction)", true, function(val)
	LG.BF_CCE.Brightness = val;
end)

--Contrast (Color Correction)
world_tab:Textbox("Contrast","(Color Correction)", true, function(val)
	LG.BF_CCE.Contrast = val;
end)

--Tint Color (Color Correction)
world_tab:Colorpicker("Tint Color", Color3.fromRGB(255,255,255), function(val)
    LG.BF_CCE.TintColor = val
end)

--  SunRays Effect
world_tab:Toggle("SunRays Effect", false, function(val)
	if val == true then
		local BFSR = Instance.new("SunRaysEffect", LG)
		BFSR.Name = "BF_SR"
		LG.BF_SR.Intensity = 0
		LG.BF_SR.Spread = 0
	else
		LG.BF_SR:Destroy()
	end
end)

--Intensity (SunRays)
world_tab:Textbox("Intensity", "(SunRays)", true, function(val)
	LG.BF_SR.Intensity = val;
end)

--Spread (SunRays)
world_tab:Textbox("Spread", "(SunRays)", true, function(val)
	LG.BF_SR.Spread = val;
end)

--  Bloom Effect
world_tab:Toggle("Bloom Effect", false, function(val)
	if val == true then
		local BFBE = Instance.new("BloomEffect", LG)
		BFBE.Name = "BF_BE"
		LG.BF_BE.Intensity = 0
		LG.BF_BE.Size = 0
		LG.BF_BE.Threshold = 0
	else
		LG.BF_BE:Destroy()
	end
end)

--Intensity (Bloom)
world_tab:Textbox("Intensity", "(Bloom)", true, function(val)
	LG.BF_BE.Intensity = val;
end)

--Size (Bloom)
world_tab:Textbox("Size", "(Bloom)", true, function(val)
	LG.BF_BE.Size = val;
end)

--Threshold (Bloom)
world_tab:Textbox("Threshold", "(Bloom)", true, function(val)
	LG.BF_BE.Threshold = val;
end)
world_tab:Seperator()

